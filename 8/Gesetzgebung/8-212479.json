    "VERKUENDUNG": "Gesetz vom 18.07.1979 - Bundesgesetzblatt Teil I 1979 Nr.4224.07.1979 S. 1061 https://www.bgbl.de/xaver/bgbl/start.xav?startbk=Bundesanzeiger_BGBl&start=//*[@attr_id='bgbl179s1061.pdf']",
    "SCHLAGWORT": [
      "Bundesgebührenordnung für Rechtsanwälte",
      "Bürgerliches Gesetzbuch",
      "Ehegesetz",
      {
        "_fundstelle": "true",
        "__cdata": "Elterliches Sorgerecht"
      },
      "Freiwillige Gerichtsbarkeit",
      "Jugendwohlfahrtsgesetz",
      "Kind",
      "Rechtspflegergesetz",
      "Vormundschaftsgericht"
