    "SCHLAGWORT": [
      "Finanzausgleich zwischen Bund und Ländern",
      "Finanzen",
      "Finanzhilfe",
      "Finanzplanung des Bundes",
      "Haushaltsausgaben",
      "Haushaltseinnahmen",
      "Haushaltspolitik",
