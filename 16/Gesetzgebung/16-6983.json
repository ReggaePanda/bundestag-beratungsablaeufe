    "SACHGEBIET": "Öffentliche Finanzen, Steuern und Abgaben",
    "SCHLAGWORT": [
      "Arbeitszimmer",
      "Bergmannsprämie",
      "Bundeskindergeldgesetz",
      "Einkommensteuer",
      "Einkommensteuergesetz",
      "Entfernungspauschale",
