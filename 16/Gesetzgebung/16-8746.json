    "SCHLAGWORT": [
      "Bagatellkriminalität",
      "Betäubungsmittelgesetz",
      "Beweis",
      "Einführungsgesetz zum Strafgesetzbuch",
      "Ermittlungsverfahren",
      "Korruption",
      "Kronzeuge",
      "Organisierte Kriminalität",
      "Sexualdelikt",
      "Strafgesetzbuch",
      "Strafrechtsänderungsgesetz",
