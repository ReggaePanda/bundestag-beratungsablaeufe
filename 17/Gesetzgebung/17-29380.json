    "SCHLAGWORT": [
      "Fahrzeugregister",
      "Gesetz über die Errichtung eines Kraftfahrt-Bundesamtes",
      "Kraftfahrsachverständigengesetz",
      "Kraftfahrt-Bundesamt",
