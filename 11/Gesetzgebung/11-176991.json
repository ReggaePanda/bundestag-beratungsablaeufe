    "SCHLAGWORT": [
      "Einkommensteuergesetz",
      "Erbschaftsteuer- und Schenkungsteuergesetz",
      "Innenpolitik",
      "Körperschaftsteuergesetz",
      "Parteienfinanzierung",
      "Parteiengesetz",
      "Spende",
